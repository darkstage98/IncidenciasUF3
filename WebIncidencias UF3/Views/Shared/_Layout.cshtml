<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title> WebIncidencias UF3 </title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Place favicon.ico in the root directory -->
    <link href="~/Content/app.css" rel="stylesheet" />
    <link href="~/Content/vendor.css" rel="stylesheet" />
    <!-- Theme initialization -->
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
</head>

<body>
    <div class="main-wrapper">
        <div class="app" id="app">
            <header class="header">
                <div class="header-block header-block-collapse hidden-lg-up">
                    <button class="collapse-btn" id="sidebar-collapse-btn">
                        <i class="fa fa-bars"></i>
                    </button>
                </div>
                <div class="header-block header-block-search hidden-sm-down">

                </div>
                @Html.Partial("_LoginPartial")
            </header>
            <aside class="sidebar">
                <div class="sidebar-container">
                    <div class="sidebar-header">
                        <div class="brand">
                            <div class="logo"> <span class="l l1"></span> <span class="l l2"></span> <span class="l l3"></span> <span class="l l4"></span> <span class="l l5"></span> </div> WebIncidencias UF3
                        </div>
                    </div>
                    <nav class="menu">
                        <ul class="nav metismenu" id="sidebar-menu">
                            <li>
                                <a href="@Url.Action("Index", "Home")"> <i class="fa fa-home"></i> Inicio </a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index", "Incidencias")"> <i class="fa fa-list-alt"></i> Incidencias </a>
                            </li>
                            <li >
                                <a href="#"> <i class="fa fa-cog"></i> Mantenimientos <i class="fa arrow"></i> </a>
                                <ul>
                                    <li>
                                        @Html.ActionLink("Usuarios", "Index", "ApplicationUsers")
                                    </li>
                                    <li>
                                        @Html.ActionLink("Perfiles", "Index", "Perfiles")
                                    </li>
                                    <li>
                                        @Html.ActionLink("Tipos de Incidencias", "Index", "TiposIncidencias")
                                    </li>
                                    <li>
                                        @Html.ActionLink("Distribuidores", "Index", "Distribuidores")
                                    </li>
                                    <li>
                                        @Html.ActionLink("Dispositivos", "Index", "Dispositivos")
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </nav>
                </div>
            </aside>
            <div class="sidebar-overlay" id="sidebar-overlay"></div>
            <article class="content static-tables-page">
                <div class="title-block">
                    <h1 class="title"> @ViewBag.Title </h1>
                </div>
                <section class="section">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="card">
                                <div class="card-block">
                                    <section class="example">
                                        @RenderBody()
                                    </section>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </article>
            <footer class="footer">
                <div class="footer-block author">
                    <p>&copy; @DateTime.Now.Year - WebIncidencias UF3</p>
                </div>
            </footer>
        </div>
    </div>
    <!-- Reference block for JS -->
    <script src="~/Scripts/vendor.js"></script>
    <script src="~/Scripts/app.js"></script>

    @RenderSection("scripts", required: false)
</body>

</html>



